<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework/schema/c"
	xmlns:p="http://www.springframework/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="player3" class="com.test.diEx06.Player">
		<constructor-arg value="チュシンス"/>
		<constructor-arg value="50"/>
		<constructor-arg>
			<list>
				<value>５番打者</value>
				<value>３塁手</value>
			</list>
		</constructor-arg>
		
		<property name="height">
			<value>186</value>
		</property>
		
		<property name="weight">
			<value>80</value>
		</property>
	</bean>

	<bean id="baseBallTeam" class="com.test.diEx06.BaseBallTeam"
		  c:manager="キムウンヨン" c:battingCoach="イスンチョル" c:pitchingCoach="ソンドンヨル"
		  p:hitter="カンジョンホ">
		  <property name="pitcher" value="リュヒョンジン"/>
	</bean>
	
</beans>
